<h1>Your Movie:</h1>

<p>@Model.ID has been registered</p>
<p>@Model.Title has been registered</p>
<p>@Model.Genre has been registered</p>
<p>@Model.Year has been registered</p>

@foreach (var actor in @Model.Actors)
{
<p>@actor</p>
}

@foreach (var director in Model.Directors)
{
<p>@director</p>
}